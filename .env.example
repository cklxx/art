# Default environment configuration for agent
# Copy to .env and adjust as needed.

# Google Gemini API (used by default for text + image generation)
GOOGLE_API_KEY=
GOOGLE_MODEL=gemini-1.5-flash
GOOGLE_IMAGE_MODEL=imagen-3.5-flash

# Optional OpenAI key if you switch clients
OPENAI_API_KEY=

# Optional OpenRouter settings
OPENROUTER_API_KEY=
OPENROUTER_MODEL=openrouter/auto
OPENROUTER_BASE_URL=https://openrouter.ai/api/v1
OPENROUTER_REFERER=
OPENROUTER_TITLE=

# Optional persistence adapters (defaults stay in-memory)
STATE_DB_PATH=:memory:
STATE_LOG_PATH=.data/state_log.jsonl
OBJECT_STORE_PATH=.data/object_store
